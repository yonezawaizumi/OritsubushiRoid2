<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
<item android:state_checked="true"
  android:drawable="@drawable/list_pressed_holo_dark" />
<item android:state_activated="true">
<layer-list>
    <item android:drawable="@android:color/black" />
    <item   android:drawable="@drawable/list_activated_holo"/>
</layer-list>
</item>
<item android:state_focused="true">
<layer-list>
    <item android:drawable="@android:color/black" />
    <item   android:drawable="@drawable/list_focused_holo"/>
</layer-list>
</item>
<item android:state_pressed="true">
<layer-list>
    <item android:drawable="@android:color/black" />
    <item   android:drawable="@drawable/list_pressed_holo_dark"/>
</layer-list>
</item>
<item android:state_selected="true">
<layer-list>
    <item android:drawable="@android:color/black" />
    <item   android:drawable="@drawable/list_selected_holo_dark"/>
</layer-list>
</item>
<item
  android:drawable="@android:color/transparent" />
</selector>
